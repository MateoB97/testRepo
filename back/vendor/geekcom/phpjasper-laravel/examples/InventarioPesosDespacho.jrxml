	<parameter name="grupo_id" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[select 
p.nombre 
,pt.almacenamiento 
,sum(i.cantidad) as peso
from inventarios i
inner join productos p on i.producto_id = p.id
inner join prod_subgrupos ps on ps.id = p.prod_subgrupo_id
inner join prod_grupos pg on pg.id = ps.prodGrupo_id
inner join producto_terminados pt on i.id = pt.invent_id

where i.tipo_invent = 2
and ($P{fecha_inicial} IS NULL OR  replace(cast( pt.created_at as date), '-', '/' ) >= $P{fecha_inicial})
and ($P{fecha_final} IS NULL OR  replace(cast( pt.created_at as date), '-', '/' ) <= $P{fecha_final})
and ($P{producto_id} IS NULL OR  p.id = $P{producto_id})
and ($P{subgrupo_id} IS NULL OR ps.id = $P{subgrupo_id})
and ($P{grupo_id} IS NULL OR pg.id = $P{grupo_id})
group by  pt.almacenamiento, p.nombre
order by pt.almacenamiento desc]]>
	</queryString>
